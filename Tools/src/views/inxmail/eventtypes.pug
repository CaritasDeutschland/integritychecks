html
    head
        title= title
    body
        include includes/head.pug
        h2= title
        if !hasContent
            div
                | No eventtypes found
        else
            div
                div
                    table(width='100%', border=1)
                        tbody
                            each item in results
                                tr
                                    td= item.name
                                    td= item.customerEmail
                                    td= item.customerMapping
                                    td= item._links.self.href
